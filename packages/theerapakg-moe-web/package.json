{
  "private": true,
  "name": "theerapakg-moe-web",
  "scripts": {
    "dev:redis:remove": "docker container stop theerapakg-moe-redis; docker container rm theerapakg-moe-redis",
    "dev:redis": "pnpm dev:redis:remove; pnpm jiti ./commands/dev_redis.ts",
    "dev:redis:chown": "sudo chown -R $(id -u -n):$(id -g -n) .redis",
    "dev:postgres:remove": "docker container stop theerapakg-moe-postgres; docker container rm theerapakg-moe-postgres",
    "dev:postgres": "pnpm dev:postgres:remove; pnpm jiti ./commands/dev_postgres.ts",
    "dev:postgres:chown": "sudo chown -R $(id -u -n):$(id -g -n) .postgres",
    "dev:psql": "docker exec -it theerapakg-moe-postgres psql -U postgres",
    "dev:drizzle:generate": "drizzle-kit generate:pg --schema=schema",
    "dev:drizzle:migrate": "pnpm jiti ./commands/drizzle_migrate.ts",
    "dev:meili:remove": "docker container stop theerapakg-moe-meili; docker container rm theerapakg-moe-meili",
    "dev:meili": "pnpm dev:meili:remove; pnpm jiti ./commands/dev_meili.ts",
    "dev:meili:chown": "sudo chown -R $(id -u -n):$(id -g -n) .meili",
    "dev:db:remove": "concurrently \"pnpm:dev:redis:remove\" \"pnpm:dev:postgres:remove\" \"pnpm:dev:meili:remove\"",
    "dev:db": "concurrently \"pnpm:dev:redis\" \"pnpm:dev:postgres\" \"pnpm:dev:meili\"",
    "dev:site": "nuxi dev",
    "dev:app": "tauri dev",
    "dev": "pnpm dev:db; concurrently --kill-others \"pnpm:dev:site\" \"pnpm:dev:app\"",
    "build:site": "nuxi build",
    "build:_app": "pnpm tauri build",
    "build:app": "pnpm generate:site && pnpm build:_app",
    "generate:site": "nuxi generate",
    "start:site": "node .output/server/index.mjs"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.1.19",
    "@monaco-editor/loader": "^1.4.0",
    "@nuxt/devtools": "^0.8.5",
    "@nuxt/kit": "^3.12.4",
    "@nuxt/test-utils-edge": "3.7.0-28195842.89ad88bd",
    "@nuxthq/ui": "^2.7.0",
    "@pinia/nuxt": "^0.4.11",
    "@rushstack/eslint-patch": "^1.7.0",
    "@tauri-apps/cli": "^1.5.9",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/dockerode": "^3.3.23",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.11.5",
    "@types/node-fetch": "^2.6.11",
    "@types/request-ip": "^0.0.38",
    "@types/whatwg-mimetype": "^3.0.2",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vueuse/core": "^10.7.2",
    "@vueuse/gesture": "2.0.0-beta.1",
    "@vueuse/nuxt": "^10.7.2",
    "argon2": "^0.31.2",
    "arktype": "1.0.19-alpha",
    "dockerode": "^3.3.5",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "esbuild": "^0.19.11",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "meilisearch": "^0.34.2",
    "mime": "^3.0.0",
    "monaco-editor": "^0.41.0",
    "nuxt": "^3.9.3",
    "nuxt-lodash": "^2.5.3",
    "pinia": "^2.1.7",
    "plotly.js-dist": "^2.33.0",
    "postcss": "^8.4.33",
    "postcss-custom-properties": "^13.3.4",
    "postgres": "^3.4.3",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "request-ip": "^3.3.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27",
    "whatwg-mimetype": "^3.0.0",
    "ws": "^8.16.0"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.5.3",
    "portal-vue": "^3.0.0"
  }
}
